    </Reference>
    <Reference Include="tflibWrapperPlus">
      <HintPath>..\Dependencies\Release\tflibWrapperPlus.dll</HintPath>
    </Reference>
    <Reference Include="tflibWrapperSharp">
      <HintPath>..\Dependencies\Release\tflibWrapperSharp.dll</HintPath>
    </Reference>
  </ItemGroup>

</Project>
